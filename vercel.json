{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["icn1"],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.4kissyou.com"
        }
      ],
      "destination": "https://4kissyou.com/:path*",
      "permanent": true
    }
  ]
}

